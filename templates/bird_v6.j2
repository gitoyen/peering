protocol bgp {{ ix_name_strip  }}_{{ neighbor_as }}{%- if session_num > 1 %}_{{ session_num }}{%- endif %}  from tpl_{{ ix_name_strip  }}_peers {
    neighbor {{ neighbor_ipv6 }} as {{ neighbor_as }};
    description "{{ ix_name }} / {{ description }}";
    {%- if limit_ipv6 %}
        {% if limit_ipv6 > 50 %}
    import limit {{ limit_ipv6 }};
        {%- endif %}
    {%- endif %}
}
